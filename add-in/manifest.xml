<?xml version="1.0" encoding="UTF-8"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:type="MailApp">
  <Id>12345678-1234-5678-1234-567812345678</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>MattDaly.org</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="ReplyCheck Add-in"/>
  <Description DefaultValue="Check for all emails in your inbox that should potentially should have been sent as a 'reply all'"/>

  <!-- Icon URLs -->
  <IconUrl DefaultValue="https://upload.wikimedia.org/wikipedia/commons/thumb/4/4f/New-email-envelope-back-symbol-in-circular-outlined-button.svg/1024px-New-email-envelope-back-symbol-in-circular-outlined-button.svg.png"/>

  <!-- Support Information -->
  <SupportUrl DefaultValue="https://www.mattdaly.org/"/>

  <!-- Properties -->
  <AppDomains>
    <AppDomain>https://www.mattdaly.org/</AppDomain>
  </AppDomains>

  <!-- Permissions -->
  <Permissions>ReadWriteMailbox</Permissions>

  <!-- Requirements -->
  <Requirements>
    <Sets>
      <Set Name="Mailbox" MinVersion="1.5"/>
    </Sets>
  </Requirements>

  <!-- End of OfficeApp -->
</OfficeApp>
